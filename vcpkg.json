{
  "name": "newleaf-demos",
  "version": "0.2.0",
  "description": "Showcase of newleaf engine capabilities",
  "homepage": "https://github.com/kafkaphoenix/newleaf_demos",
  "license": "MIT",
  "dependencies": [
    "assimp",
    "entt",
    {
      "name": "glad",
      "features": [
        "gl-api-46"
      ]
    },
    "glfw3",
    "glm",
    {
      "name": "imgui",
      "features": [
        "docking-experimental",
        "glfw-binding",
        "opengl3-binding"
      ]
    },
    "nlohmann-json",
    {
      "name": "spdlog",
      "features": [
        "wchar"
      ]
    },
    "stb"
  ],
  "overrides": [
    {"name": "assimp", "version": "6.0.2#0"},
    {"name": "entt", "version": "3.15.0#0"},
    {"name": "glad", "version": "0.1.36#0"},
    {"name": "glfw3", "version": "3.4#1"},
    {"name": "glm", "version": "1.0.1#3"},
    {"name": "imgui", "version": "1.91.9#0"},
    {"name": "nlohmann-json", "version": "3.12.0#1"},
    {"name": "spdlog", "version": "1.15.3#0"},
    {"name": "stb", "version": "2024-07-29#1"}
  ]
}
